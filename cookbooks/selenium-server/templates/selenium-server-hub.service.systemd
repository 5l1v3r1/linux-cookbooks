[Unit]
Description=Selenium Server (Hub)
After=network.target

[Install]
WantedBy=multi-user.target

[Service]
Type=simple

LimitNOFILE=524288
LimitNPROC=524288

Restart=always
RestartSec=10

TimeoutSec=30

User=__USER_NAME__
Group=__GROUP_NAME__

ExecStart=/bin/bash -e -c '\
    java -jar \'__INSTALL_FILE__\' -role \'hub\' -hubConfig \'__CONFIG_FILE__\' \
'