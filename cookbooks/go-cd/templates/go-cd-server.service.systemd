[Unit]
Description=Go-CD Server
After=network.target

[Install]
WantedBy=multi-user.target

[Service]
Type=simple

LimitNOFILE=524288
LimitNPROC=524288

Restart=always
RestartSec=10

TimeoutSec=30

User=__USER_NAME__
Group=__GROUP_NAME__

Environment='HOME=__GO_HOME_FOLDER__'

WorkingDirectory=__SERVER_INSTALL_FOLDER__

ExecStart=/bin/bash -c -e 'java -jar 'go.jar''