description 'Kibana Server'

start on (local-filesystems and net-device-up IFACE!=lo)
stop on runlevel [!2345]

limit nofile 524288 524288
limit nproc 524288 524288

respawn
respawn limit 10 5

kill timeout 20

setuid '__USER_NAME__'
setgid '__GROUP_NAME__'

chdir '__INSTALL_FOLDER__'
exec '__INSTALL_FOLDER__/bin/kibana'