[Unit]
Description=Kibana Server
After=network.target

[Install]
WantedBy=multi-user.target

[Service]
Type=simple

LimitNOFILE=524288
LimitNPROC=524288

Restart=always
RestartSec=10

TimeoutStartSec=20
TimeoutStopSec=20

User=__USER_NAME__
Group=__GROUP_NAME__

WorkingDirectory=__INSTALL_FOLDER__

ExecStart='__INSTALL_FOLDER__/bin/kibana'